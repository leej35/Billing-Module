<h1>Listing pay_types</h1>

<table>
  <tr>
    <th>String</th>
  </tr>

<% @pay_types.each do |pay_type| %>
  <tr>
    <td><%=h pay_type.name %></td>
    <td><%= link_to 'Show', pay_type %></td>
    <td><%= link_to 'Edit', edit_pay_type_path(pay_type) %></td>
    <td><%= link_to 'Destroy', pay_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New pay_type', new_pay_type_path %>