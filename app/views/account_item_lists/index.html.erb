<h1>Listing account_item_lists</h1>

<table>
  <tr>
    <th>Account</th>
    <th>Item</th>
  </tr>

<% @account_item_lists.each do |account_item_list| %>
  <tr>
    <td><%=h account_item_list.account_id %></td>
    <td><%=h account_item_list.item_id %></td>
    <td><%= link_to 'Show', account_item_list %></td>
    <td><%= link_to 'Edit', edit_account_item_list_path(account_item_list) %></td>
    <td><%= link_to 'Destroy', account_item_list, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New account_item_list', new_account_item_list_path %>